{
	"name": "coveragepy-jtag",
	"displayName": "CoveragePy Jtag",
	"description": "View python coverage variation in real time.",
	"version": "0.0.1",
	"publisher": "lyyyuna",
	"repository": {
		"url": "https://github.com/lyyyuna/coveragepy-vscode"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.switch",
				"title": "Enable/Disable coverage report"
			}
		],
		"configuration": {
			"title": "CoveragePy Jtag",
			"properties": {
				"coveragepy.serverUrl": {
					"type": "string",
					"default": "http://127.0.0.1:8081",
					"items": {
						"type": "string"
					},
					"description": "Specify the coveragepy jtag server url."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"axios": "^0.19.2"
	}
}
